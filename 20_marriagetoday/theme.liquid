<head>

  <!-- Basic page needs ================================================== -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  {% comment %}
     Include CSS frameworks with CDNs
  {% endcomment %}
  {{ '//cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css' | stylesheet_tag }}
  {{ '//maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css' | stylesheet_tag }}
  {{ '//cdn.jsdelivr.net/jquery.slick/1.5.7/slick.css' | stylesheet_tag }}
  {{ 'main.css' | stylesheet_tag }}
  {{ 'slick.css' | stylesheet_tag }}
  {{ 'player.css' | stylesheet_tag }}
  {% comment %}
     Always include application javascript file, this file provide jquery and some cool helpers
  {% endcomment %}
  {{ 'application.js' | javascript_tag }}
  {{ 'main.js' | javascript_tag }}
  {% comment %}
     Include JWPlayer.
  {% endcomment %}
  {{ 'http://content.jwplatform.com/libraries/D1C0qKP3.js' | javascript_tag }}
  {% csrf_meta_tags %}
  {% include 'seo_data' %}
</head>
<body>
  <div class="page-wrap">
    {% include 'header' %}
    <div class="content">
      {{ content_for_layout }}
    </div>
  </div>
  <div class="footer">
    {% include 'footer' %}
  </div>
  {{ '//cdn.jsdelivr.net/jquery.slick/1.5.7/slick.min.js' | javascript_tag }}
</body>
</html>
